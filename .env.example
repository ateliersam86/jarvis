# Jarvis Server Configuration
# Copy this file to .env.local and fill in your values

# Your deployment server IP or hostname
JARVIS_SERVER_IP=your-server-ip

# SSH connection string for deployment
JARVIS_SSH_HOST=user@your-server-ip

# Remote deployment path
JARVIS_DEPLOY_PATH=/path/to/jarvis

# Public URL of your Jarvis instance
JARVIS_PUBLIC_URL=https://your-domain.com

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/jarvis"

# NextAuth
NEXTAUTH_SECRET=your-secret-key
NEXTAUTH_URL=https://your-domain.com
